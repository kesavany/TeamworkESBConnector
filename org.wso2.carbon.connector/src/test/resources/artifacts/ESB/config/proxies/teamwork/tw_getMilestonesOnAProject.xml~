<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="tw_getMilestonesOnAProject"
       transports="https http"
       startOnLoad="true"
       trace="disable">
   <description/>
   <target>
      <inSequence>
         <property name="apiUrl" expression="json-eval($.apiUrl)"/>
         <property name="client_id" expression="json-eval($.client_id)"/>
         <property name="project_id" expression="json-eval($.project_id)"/>
	  <property name="find" expression="json-eval($.find)"/>
         <property name="getProgress" expression="json-eval($.getProgress)"/>
         <teamwork.init>
            <apiUrl>{$ctx:apiUrl}</apiUrl>
            <client_id>{$ctx:client_id}</client_id>
         </teamwork.init>
         <teamwork.getMilestonesOnAProject>
            <project_id>{$ctx:project_id}</project_id>
	    <find>{$ctx:find}</find>
            <getProgress>{$ctx:getProgress}</getProgress>
         </teamwork.getMilestonesOnAProject>
         <respond/>
      </inSequence>
      <outSequence/>
      <faultSequence/>
   </target>
</proxy>
