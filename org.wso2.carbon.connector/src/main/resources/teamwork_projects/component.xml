<?xml version="1.0" encoding="UTF-8"?>
<component name="teamwork_projects" type="synapse/template">
    <subComponents>
        <component name="createProject">
            <file>createProject.xml</file>
            <description>Create a single project.</description>
        </component>
        <component name="updateProject">
            <file>updateProject.xml</file>
            <description>Modifies a single project.</description>
        </component>
        <component name="deleteProject">
            <file>deleteProject.xml</file>
            <description>Delete a project.</description>
        </component>
        <component name="getProject">
            <file>getProject.xml</file>
            <description>Returns a single project identified by its ID.</description>
        </component>
        <component name="getAllProjects">
            <file>getAllProjects.xml</file>
            <description>Retrieves all accessible projects.</description>
        </component>
        <component name="getStarredProjects">
            <file>getStarredProjects.xml</file>
            <description>Retrieve all the projects, which have been starred.</description>
        </component>
        <component name="starAProject">
            <file>starAProject.xml</file>
            <description>Adds a project to the list of favourite projects.</description>
        </component>
        <component name="unstarAProject">
            <file>unstarAProject.xml</file>
            <description>Removes a project from the list of favourite projects.</description>
        </component>
    </subComponents>
